{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/health",
        "dest": "dist/index.js",
        "methods": ["GET"]
      },
      {
        "src": "/api/(.*)",
        "dest": "dist/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ],
    "functions": {
      "dist/index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "crons": [
      {
        "path": "/health",
        "schedule": "*/10 * * * *"
      }
    ]
}